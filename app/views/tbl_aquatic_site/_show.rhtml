<h4><%= @record.name || 'Unnamed Aquatic Site' -%></h4>

<% if @record.incorporated %>
  <div class="messages-container">
    <div class="incorporated-message">
      <h2>This aquatic site has incorporated into the Aquatic Data Warehouse</h2>
      <ul>
        <li>Edit and Delete functions are disabled</li>
        <li>Adding and editing of attached activities is allowed</li>
      </ul>
    </div>
  </div>
<% end %>

<%= render :partial => 'show_columns', :locals => {:columns => active_scaffold_config.show.columns} -%>

<p class="form-footer">
  <%= link_to as_('Close'), params_for(:action => 'list'), :class => 'cancel' %>
  <%= loading_indicator_tag(:action => :create, :id => params[:id]) %>
</p>